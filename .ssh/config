# Enable multiple .ssh keys
# git remote add marvin git@git-marvin:whatever.git
# git remote add alias1 git@git-alias1:whatever.git

Host git-marvin
  HostName github.com
  User git
  IdentityFile /home/pili/.ssh/id_rsa
  IdentitiesOnly yes

Host git-alias1
  HostName github.com
  User git
  IdentityFile /home/pili/.ssh/alias1
  IdentitiesOnly yes
